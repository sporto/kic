elm:
  elm make ./src/SignIn.elm ./src/SignUp.elm ./src/Admin.elm ./src/Investor.elm --output=src/elm-out/app.js

build:
  yarn ts-node ./node_modules/.bin/webpack --reporter basic --config webpack.dev.ts

build-prod:
  yarn ts-node ./node_modules/.bin/webpack --reporter basic --config webpack.prod.ts

start:
  yarn ts-node ./node_modules/.bin/webpack-serve ./webpack.dev.ts
